from "./commitment.zok" import main as commitment;
from "./threshold.zok" import main as threshold;

def main(private u8[26] input, private u8[32] nonce, private u16 creditScore, u16 t) -> (u32[8], bool){
    u32[8] id = commitment(input, nonce);
    bool tCheck = threshold(creditScore, t);
    return (id,tCheck) ;
}